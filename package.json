{
  "name": "keepix-react-simple-terminal-ui",
  "version": "1.0.2",
  "description": "a simple interactive terminal ui react component that allows you to programmatically emulate terminal behaviour.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Keepix-labs/react-simple-terminal-ui.git"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dist/styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    }
  },
  "scripts": {
    "build": "tsc && cp src/*.css dist"
  },
  "keywords": [
    "terminal",
    "unix",
    "console",
    "cmd",
    "xterm",
    "cli"
  ],
  "author": "movcmpret",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "typescript": "^5.3.2"
  },
  "dependencies": {
  }
}
